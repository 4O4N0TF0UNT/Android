<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/register_bg"
    tools:context=".activity_33.zlyloginActivity">
<include layout="@layout/main_title_bar"></include>
    <ImageView
        style="@style/userImage"
        android:id="@+id/iv_head"/>
    <EditText
        style="@style/reg_login_EditText"
        android:id="@+id/et_user_name_33"
        android:layout_marginTop="35dp"
        android:background="@drawable/register_user_name_bg"
        android:drawableLeft="@drawable/user_name_icon"
        android:layout_marginRight="35dp"
        android:hint="请输入用户"
        />
    <EditText
        style="@style/reg_login_EditText"
        android:id="@+id/et_psw_33"
        android:background="@drawable/register_psw_again_bg"
        android:drawableLeft="@drawable/psw_icon"
        android:layout_marginRight="35dp"
        android:hint="请输入密码"
        />
    <Button
        android:id="@+id/btn_login_33"
        style="@style/reg_login_Button"
        android:text="登  录" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="35dp"
        android:layout_marginRight="35dp"
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_reg_33"
            style="@style/login_TextView"
            android:text="立即注册"
            />
        <TextView
            android:id="@+id/tv_find_psw_33"
            style="@style/login_TextView"
            android:text="找回密码？"
            />
    </LinearLayout>
</LinearLayout>
